<div class="author">
	<h1 class="author__heading">
		Search Results
	</h1>
</div>

<section class="articles">
	<ul class="article-ul">
      <% if(data.length){ %>
        <% data.forEach(post=>{ %>
        <li>
          <a href="/post/<%= post._id %>">
            <span><%= post.title %></span>
            <span class="article-list__date"><%= post.createdAt.toDateString(); %></span>
          </a>
        </li>
        <% }) %>
      <% }else { %>
        <div class="author">No Result Found !</div>
      <% } %>
  </ul>
</section>